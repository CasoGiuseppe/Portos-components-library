{
    "name": "vue-components-noatum",
    "version": "1.2.19",
    "type": "module",
    "description": "Library Components Portos",
    "main": "dist/vue-components-noatum.umd.js",
    "module": "dist/vue-components-noatum.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/vue-components-noatum.es.js",
            "require": "./dist/vue-components-noatum.umd.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "files": [
        "dist",
        "src",
        "!src/stories",
        "!src/App.vue",
        "!src/main.ts"
    ],
    "keywords": [
        "vue",
        "components",
        "ui",
        "library",
        "design system"
    ],
    "author": "Carlos Zorrilla",
    "license": "ISC",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@storybook/components": "^8.0.8",
        "@vueuse/components": "^10.9.0",
        "storybook-vue3-router": "^5.0.0",
        "vue": "^3.4.15",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.3",
        "@storybook/addon-essentials": "^7.6.17",
        "@storybook/addon-interactions": "^7.6.17",
        "@storybook/addon-links": "^7.6.17",
        "@storybook/blocks": "^7.6.17",
        "@storybook/test": "^7.6.17",
        "@storybook/vue3": "^7.6.17",
        "@storybook/vue3-vite": "^7.6.17",
        "@tsconfig/node20": "^20.1.2",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^20.11.10",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.4",
        "@vue/tsconfig": "^0.5.1",
        "eslint": "^8.49.0",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-vue": "^9.17.0",
        "jsdom": "^24.0.0",
        "npm-run-all2": "^6.1.1",
        "prettier": "^3.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.71.1",
        "storybook": "^7.6.17",
        "typescript": "~5.3.0",
        "vite": "^5.0.11",
        "vite-plugin-dts": "^3.7.3",
        "vitest": "^1.2.2",
        "vue-tsc": "^1.8.27"
    }
}
